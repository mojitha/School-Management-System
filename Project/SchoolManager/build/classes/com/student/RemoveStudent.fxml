<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.student.RemoveStudentController">
    <stylesheets>
        <URL value="@Studentviewstyles.css" />
    </stylesheets>
